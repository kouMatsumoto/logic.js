{
  "name": "logical-validation",
  "version": "0.1.0",
  "description": "Validation with Logic programming for TypeScript",
  "main": "./dist/commonjs/index.js",
  "module": "./dist/es/index.js",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsc --noEmit && rollup --config",
    "check": "npm-run-all --parallel format lint --serial diff --parallel build test",
    "diff": "git diff --exit-code --name-only ./",
    "format": "prettier --write --ignore-path=.gitignore ./",
    "lint": "eslint --ext=.ts --fix src",
    "prepack": "npm ci && rm -fr dist && npm run build",
    "test": "jest --ci --silent --detectOpenHandles --forceExit"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kouMatsumoto/logical-validation.git"
  },
  "keywords": [],
  "author": "Kou Matsumoto <developer@kou-matsumoto.jp>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/kouMatsumoto/logical-validation/issues"
  },
  "homepage": "https://github.com/kouMatsumoto/logical-validation#readme",
  "devDependencies": {
    "@babel/preset-env": "^7.15.4",
    "@rollup/plugin-babel": "^5.3.0",
    "@rollup/plugin-typescript": "^8.2.5",
    "@types/jest": "^27.0.1",
    "@types/node": "^16.7.10",
    "@typescript-eslint/eslint-plugin": "^4.30.0",
    "@typescript-eslint/parser": "^4.30.0",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "jest": "^27.1.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.3.2",
    "rollup": "^2.56.3",
    "ts-essentials": "^8.1.0",
    "ts-jest": "^27.0.5",
    "typescript": "^4.3.5"
  },
  "dependencies": {
    "utility-types": "^3.10.0"
  }
}
